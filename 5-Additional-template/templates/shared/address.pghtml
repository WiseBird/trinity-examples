{{ define "Address" }}
    <table width="100%" border="0" cellpadding="0" cellspacing="5" style="">
        <tr>
			<td width="100px">
				Street:
			</td>
			<td>
				{{ .Street }}
			</td>
		</tr>
		<tr>
			<td>
				City:
			</td>
			<td>
				{{ .City }}
			</td>
		</tr>
		<tr>
			<td>
				State
			</td>
			<td>
				{{ .State }}
			</td>
		</tr>
		<tr>
			<td>
				Country
			</td>
			<td>
				{{ .Country }}
			</td>
		</tr>
	</table>
{{ end }}